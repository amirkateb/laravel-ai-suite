{
  "name": "amirkateb/laravel-ai-suite",
  "description": "Unified AI suite for Laravel with multi-provider drivers, fallback, dynamic model listing, pricing, chat, OCR, embeddings, images, audio, tools, and fine-tuning.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Amir Kateb",
      "email": "amveks43@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "AmirKateb\\AiSuite\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "AmirKateb\\AiSuite\\Providers\\AiSuiteServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.5|^11.0"
  }
}